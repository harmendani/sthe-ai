{
  "compilerOptions": {
    "target": "es6",
    "module": "es2022",
    "moduleResolution": "bundler",
    "lib": [
      "ES6"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
  }
}